{
  "scripts": {
    "build": "webpack --progress --config assets/config/webpack.prod.js",
    "lint": "npm run -s lint:scripts && npm run -s lint:styles",
    "lint:scripts": "eslint assets/scripts",
    "lint:styles": "stylelint \"assets/styles/**/*.{css,sass,scss}\"",
    "rmdist": "rimraf dist",
    "sprite": "svg-sprite --config assets/sprite.json 'assets/images/svg/*.svg'",
    "start": "webpack --watch --config assets/config/webpack.dev.js"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-webpack-plugin": "^3.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^2.2.2",
    "postcss-loader": "^6.1.1",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.39.0",
    "sass-loader": "^12.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "swc-loader": "^0.1.14",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-remove-empty-scripts": "^0.7.1"
